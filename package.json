{
  "name": "zi-src",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "trunk": "trunk",
    "crowdin": "crowdin",
    "wrangler": "wrangler",
    "clean": "pnpm exec rm -rf node_modules pnpm-lock.yaml",
    "publish": "wrangler publish",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm audit",
    "cdn:dev": "pnpm --filter @workers/cdn -- dev",
    "r2:dev": "pnpm --filter @workers/r2-store -- dev",
    "hot-link:dev": "pnpm --filter @workers/hot-link_protection -- dev",
    "fmt": "trunk fmt",
    "check": "trunk check",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@zi-src/eslint": "*",
    "@workers/hot-link_protection": "*",
    "@workers/r2-store": "*",
    "@workers/cdn": "*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.16.0",
    "@trunkio/launcher": "1.2.1",
    "wrangler": "2.1.9",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16",
    "yarn": "3.2.3",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@7.12.2"
}
